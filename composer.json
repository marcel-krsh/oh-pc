{
    "name": "greenwood360/allita_pcsv",
    "description": "Allita PCSV",
    "keywords": [],
    "license": "Closed",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "aloha/twilio": "^4.0",
        "anhskohbo/no-captcha": "^3.0",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "barryvdh/laravel-snappy": "^0.4.4",
        "box/spout": "^2.7",
        "chumper/zipper": "^1.0",
        "divineomega/php-dot-net-ticks": "^1.0",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "^6.3",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4",
        "intervention/image": "^2.3",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "phpoffice/phpspreadsheet": "^1.6",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.3",
        "renatomarinho/laravel-page-speed": "^1.8",
        "sentry/sentry-laravel": "1.4.1",
        "soumen-dey/laravel-user-agent": "^1.0",
        "staudenmeir/eloquent-json-relations": "^1.1",
        "torann/geoip": "^1.0",
        "tremby/laravel-git-version": "^1.1",
        "yajra/laravel-datatables": "^1.0"
    },
    "require-dev": {
        "aloha/twilio": "^4.0",
        "barryvdh/laravel-debugbar": "^3.1",
        "beyondcode/laravel-dump-server": "~1.0",
        "codedungeon/phpunit-result-printer": "^0.8",
        "doctrine/dbal": "v2.5.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "^2.0",
        "laravel/homestead": "^7.20",
        "mockery/mockery": "0.9.*",
        "nunomaduro/collision": "~1.1",
        "phpunit/phpunit": "~6.0",
        "wnx/laravel-stats": "^1.7"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/AllitaBlightManagerHelpers.php",
            "app/Helpers/StatusHelper.php",
            "app/Helpers/helpers.php",
            "app/Helpers/GuideHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
